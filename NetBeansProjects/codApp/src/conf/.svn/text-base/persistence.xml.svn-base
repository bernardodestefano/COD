    <class>bean.Visite</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost/cod"/>
      <property name="javax.persistence.jdbc.password" value="cod"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="cod"/>
    </properties>
  </persistence-unit>
</persistence>
