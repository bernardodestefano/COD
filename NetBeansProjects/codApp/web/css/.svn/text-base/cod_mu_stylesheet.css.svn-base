body
{
	font-family: Helvetica Neue, Helvetica, sans-serif;
	font-size: 14px;
	background-color:  #EBEDEC;
}

#login
{
	width: 250px;
	height: 100px;
	background-color: #DDDDDD;
	text-align: center;
	margin-top: 50%;
	top: -100px;
	position:absolute;
	margin-left: 50%;
	left:-116px;
	padding-top: 20px;
	padding-bottom: 8px;
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-top:1px solid #FFF;
	border-left:1px solid #939393;
	border-right:1px solid #939393;
	border-bottom:1px solid #939393;
	-webkit-box-shadow: 0px -1px 1px #939393;
}
#abilita
{
	width: 250px;
	height: 12px;
	text-align: center;
	margin-top: 50%;
	top: 350px;
	position:absolute;
	margin-left: 50%;
	left:-117px;
	padding-top: 20px;
	padding-bottom: 8px;
}

#dottore
{
	width: 300px;
	height: auto;
	background-color: #DDDDDD;
	text-align: center;
	margin: 0 auto;
	-webkit-border-radius: 6px;
	-moz-border-radius: 6px;
	border-top:1px solid #FFF;
	border-left:1px solid #939393;
	border-right:1px solid #939393;
	border-bottom:1px solid #939393;
	-webkit-box-shadow: 0px -1px 1px #939393;
}

.login-text{
	width: 200px;
	height: 25px;
	font-size:14px;
	margin-bottom: 10px;
	border:1px solid #939393;
	color:#333333;
	padding: 5px;
}

input[type=submit]{
	border:1px solid #939393;
	color:#333333;
	background:CCCCCC;
	font-size: 13px;
	margin-bottom: 10px;
}
